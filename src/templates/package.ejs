{
  "type": "module",
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "Generated with ForgeAPI - Modular API Generator",
  "main": "app.js",
  "scripts": {
<%  const scriptKeys = Object.keys(scripts);
  scriptKeys.forEach((key, i) => {
%>    "<%= key %>": "<%= scripts[key] %>"<%= i < scriptKeys.length - 1 ? ',' : '' %>
<% })%>  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
<%
  const keys = Object.keys(dependencies);
  keys.forEach((name, i) => {%>    "<%= name %>": "<%= dependencies[name] %>"<%= i < keys.length - 1 ? ',' : '' %>
<% }) %>  },
  "devDependencies": {
<%  const devKeys = Object.keys(devDependencies);
  devKeys.forEach((name, i) => {%>    "<%= name %>": "<%= devDependencies[name] %>"<%= i < devKeys.length - 1 ? ',' : '' %>
<% }) %>  },
  "generator": {
    "name": "ForgeAPI",
    "version": "0.1.3",
    "auth": "<%= authType %>",
    "db": "<%= dbType %>"
  }
}
